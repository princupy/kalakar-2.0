        "discord.js",
        "eris",
        "discordeno.js",
        "bot",
        "bots",
        "cluster",
        "framework"
    ],
    "author": "meister03",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/meister03/discord-hybrid-sharding/issues"
    },
