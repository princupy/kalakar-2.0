    "devDependencies": {
        "@types/jest": "^29.5.12",
        "@types/node": "^20.12.2",
        "@typescript-eslint/eslint-plugin": "^7.5.0",
        "@typescript-eslint/parser": "^7.5.0",
        "clean-jsdoc-theme": "^4.2.18",
        "discord.js": "^14.14.1",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-jsdoc": "^48.2.2",
        "eslint-plugin-prettier": "^5.1.3",
        "jest": "^29.7.0",
        "jsdoc": "^4.0.2",
        "prettier": "^3.2.5",
        "ts-jest": "^29.1.2",
        "typescript": "^5.4.3"
