    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "scripts": {
        "build": "npx tsc",
        "lint": "eslint src/**/**.ts",
        "lint:fix": "eslint src/**/**.ts --fix",
        "format": "prettier --check .",
        "format:fix": "prettier --write .",
        "generate-docs": "jsdoc -c ./.jsdoc.conf.json"
